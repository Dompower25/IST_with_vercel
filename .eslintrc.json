{
  "extends": "next/core-web-vitals",
  "plugins": ["prettier"],
  "rules": {
    "import/no-unresolved": [
      2,
      {
        "caseSensitiveStrict": true
      }
    ]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
